{
  "family": "qtable-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "2048",
  "executionRoleArn": "arn:aws:iam::305448253656:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "qtable-container",
      "image": "305448253656.dkr.ecr.ap-northeast-2.amazonaws.com/qtable-repo:latest",
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/qtable-task",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "prod"
        },
        {
          "name": "DB_HOST",
          "value": "qtable-db.crqya2um8g7x.ap-northeast-2.rds.amazonaws.com"
        },
        {
          "name": "DB_PORT",
          "value": "3306"
        },
        {
          "name": "DB_NAME",
          "value": "qtable_db"
        },
        {
          "name": "DB_USERNAME",
          "value": "admin"
        },
        {
          "name": "DB_PASSWORD",
          "value": "Test1234!"
        }
      ]
    }
  ]
}